#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-email/dist/cli/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-email/dist/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-email/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-email/dist/cli/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-email/dist/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-email/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/react-email@4.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.9_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/Users/nit/Developer/apps/_template/web/turbokit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/cli/index.js" "$@"
fi
