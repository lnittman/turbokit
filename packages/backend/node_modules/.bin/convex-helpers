#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nit/Developer/_template/xyz/turbokit/node_modules/.pnpm/convex-helpers@0.1.104_convex@1.26.2_@clerk+clerk-react@5.31.1_react-dom@19.1.0_react@1_71367e88eb5b14e499a1ef4a9f90c5fd/node_modules/convex-helpers/node_modules:/Users/nit/Developer/_template/xyz/turbokit/node_modules/.pnpm/convex-helpers@0.1.104_convex@1.26.2_@clerk+clerk-react@5.31.1_react-dom@19.1.0_react@1_71367e88eb5b14e499a1ef4a9f90c5fd/node_modules:/Users/nit/Developer/_template/xyz/turbokit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nit/Developer/_template/xyz/turbokit/node_modules/.pnpm/convex-helpers@0.1.104_convex@1.26.2_@clerk+clerk-react@5.31.1_react-dom@19.1.0_react@1_71367e88eb5b14e499a1ef4a9f90c5fd/node_modules/convex-helpers/node_modules:/Users/nit/Developer/_template/xyz/turbokit/node_modules/.pnpm/convex-helpers@0.1.104_convex@1.26.2_@clerk+clerk-react@5.31.1_react-dom@19.1.0_react@1_71367e88eb5b14e499a1ef4a9f90c5fd/node_modules:/Users/nit/Developer/_template/xyz/turbokit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/convex-helpers@0.1.104_convex@1.26.2_@clerk+clerk-react@5.31.1_react-dom@19.1.0_react@1_71367e88eb5b14e499a1ef4a9f90c5fd/node_modules/convex-helpers/bin.cjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/convex-helpers@0.1.104_convex@1.26.2_@clerk+clerk-react@5.31.1_react-dom@19.1.0_react@1_71367e88eb5b14e499a1ef4a9f90c5fd/node_modules/convex-helpers/bin.cjs" "$@"
fi
